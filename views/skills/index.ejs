<%- include('../partials/header') %>

<h1>Express-Dev-Skills</h1>
<a href="/skills/new">Add Skill</a>


<ul>
  <% skills.forEach(function(s) { %>
  <li>
    <form action="/skills/<%= s.id %>?_method=DELETE" method="POST">
      <button type="submit">x</button>
  </form>
    <a href="/skills/<%= s.id %>"> <%= s.skill %> </a>
     <%= s.name %> - <%= s.description %> - <%= s.level %>
  </li>
  <% }); %>
</ul>

<%- include('../partials/footer') %>
